{
  "license": "ISC",
  "version": "1.0.0",
  "name": "appium-wdio",
  "author": "RVÐ-Projects",
  "description": "An Appium WebDriverIO setup for hybrid apps (web, mobile)",
  "scripts": {
    "wdio:install": "npx @wdio/cli init",
    "appium:install": "npx appium-installer",
    "appium:start": "npx appium",
    "test:post": "rm -rf allure-report/ && rm -rf logs/",
    "test:android": "npm run test:post && npx wdio configs/myApp/wdio.android.app.conf.ts",
    "test:ios": "npm run test:post && npx wdio configs/myApp/wdio.ios.app.conf.ts",
    "allure:open": "npx allure open"
  },
  "dependencies": {
    "appium-wdio": "file:"
  },
  "devDependencies": {
    "@appium/execute-driver-plugin": "^3.0.36",
    "@appium/images-plugin": "^3.0.21",
    "@appium/relaxed-caps-plugin": "^1.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@wdio/allure-reporter": "^9.2.2",
    "@wdio/appium-service": "^9.2.11",
    "@wdio/cli": "^9.2.11",
    "@wdio/local-runner": "^9.2.11",
    "@wdio/mocha-framework": "^9.2.8",
    "@wdio/spec-reporter": "^9.2.8",
    "allure-commandline": "^2.32.0",
    "appium": "^2.12.1",
    "appium-altunity-plugin": "^1.3.3",
    "appium-chromium-driver": "^1.4.4",
    "appium-dashboard": "^2.0.3",
    "appium-ddlog-plugin": "^0.1.4",
    "appium-device-farm": "^9.4.5",
    "appium-espresso-driver": "^3.5.0",
    "appium-geckodriver": "^1.4.0",
    "appium-gestures-plugin": "^4.0.1",
    "appium-installer": "^3.0.0",
    "appium-mac2-driver": "^1.20.2",
    "appium-ocr-plugin": "^0.2.2",
    "appium-otel-plugin": "^0.1.3",
    "appium-reporter-plugin": "^1.1.0-beta.6",
    "appium-safari-driver": "^3.5.18",
    "appium-uiautomator2-driver": "^3.8.1",
    "appium-wait-plugin": "^3.0.2",
    "appium-windows-driver": "^3.0.2",
    "appium-xcuitest-driver": "^7.28.3",
    "chai": "^5.1.2",
